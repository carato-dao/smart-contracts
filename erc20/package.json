{
    "name": "carato-erc20",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "ganache-cli": "ganache-cli -d -p 7545 --gasLimit=10000000",
        "extract-abi": "truffle-export-abi -d ./build/contracts/ -o ./abi.json -v",
        "test": "truffle test",
        "ganache:deploy": "sudo rm -rf build && sudo truffle deploy --reset && sudo npm run extract-abi",
        "ganache:details": "node ganache/details.js",
        "ganache:mint": "node ganache/mint.js",
        "ganache:lock": "node ganache/lock.js",
        "ganache:transfer": "node ganache/transfer.js",
        "ganache:burn": "node ganache/burn.js",
        "quadrans:deploy": "sudo rm -rf build && sudo truffle deploy --network quadrans --reset --verbose-rpc && sudo npm run extract-abi",
        "quadrans:details": "node quadrans/details.js",
        "quadrans:mint": "node quadrans/mint.js",
        "quadrans:transfer": "node quadrans/transfer.js",
        "ticino:deploy": "sudo rm -rf build && sudo truffle deploy --network ticino --reset --verbose-rpc && sudo npm run extract-abi",
        "ticino:details": "node ticino/details.js",
        "ticino:mint": "node ticino/mint.js"
    },
    "author": "turinglabs",
    "license": "MIT",
    "description": "",
    "_id": "carato-erc20@1.0.0",
    "dependencies": {
        "@openzeppelin/contracts": "^4.1.0",
        "@truffle/hdwallet-provider": "^1.4.0",
        "dotenv": "^8.2.0",
        "web3": "^1.3.6"
    },
    "devDependencies": {
        "solc": "^0.8.4",
        "truffle-export-abi": "^1.0.1"
    }
}